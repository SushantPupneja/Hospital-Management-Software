import java.util.*;
import java.awt.*;





class Hospital {


	

		public static void main (String args[])	{


		Frame f1 = new Frame("New Registration");
		f1.setVisible(true);
		f1.setSize(600,125);
		f1.setLayout(new GridLayout(4,4));
		f1.setBackground(Color.ORANGE);
		

		Label lblOne = new Label("Patient ID:-");
		TextField patientid = new TextField(20);
		
		 f1.add(lblOne);
		 f1.add(patientid);

		
		Label lbltwo = new Label("Address 1:-");
		TextField address1 = new TextField(20);
		
		 f1.add(lbltwo);
		 f1.add(address1);

		
		Label lblthree = new Label("Patient Name:-");
		TextField patientname = new TextField(20);
		
		 f1.add(lblthree);
		 f1.add(patientname);


		Label lblfour = new Label ("Address 2:-");
		TextField address2 = new TextField(20);
		
		 f1.add(lblfour);
		 f1.add(address2);

		
		Label lblfive = new Label ("Sex");
		TextField sex = new TextField(6);
		
		 f1.add(lblfive);
		 f1.add(sex);


		Label lblsix = new Label ("Address3:-");
		TextField address3 = new TextField(20);
		
		 f1.add(lblsix);
		 f1.add(address3);

		Label lblseven = new Label ("Relative");
		TextField relative = new TextField(20);
		
		 f1.add(lblseven);
		 f1.add(relative);

		Label lbleight = new Label ("Country");
		TextField country = new TextField(20);
		
		 f1.add(lbleight);
		 f1.add(country);
